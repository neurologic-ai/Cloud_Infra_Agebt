# === Cloud Infra Agent Environment ===

# Whether to use Vertex AI or not (TRUE/FALSE)
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# Google API Key (replace with your real key in .env, never commit it here)
GOOGLE_API_KEY=your-google-api-key

# OpenAI API Key
OPENAI_API_KEY=your-openai-key

# Base data directory for samples and outputs
CLOUD_INFRA_DATA_DIR=cloud_infra_agent/Data